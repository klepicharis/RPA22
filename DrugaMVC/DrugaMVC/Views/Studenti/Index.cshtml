@model IEnumerable<DrugaMVC.Models.student>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @*Ime*@
            @Html.DisplayNameFor(model => model.Studentname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Age)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @*@item.Studentname*@
            @Html.DisplayFor(modelItem => item.Studentname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Age)
        </td>
        <td>
            @Html.ActionLink("Uredi", "Edit", new { id=item.Studentid }) |
            @Html.ActionLink("Podrobnosti", "Details", new { id=item.Studentid }) |
            @Html.ActionLink("Briši", "Delete", new { id=item.Studentid })
        </td>
    </tr>
}

</table>
